          </li>
          <!-- Cart -->
          <li class="nav-item d-none d-md-flex align-items-center hvr-wobble-horizontal">
            <a href="#" class="material-icons nav-link d-none d-md-block text-secondary">
              shopping_cart
            </a>
            <a class="nav-link h5 mb-0 p-0" href="#">Cart(3)</a>
