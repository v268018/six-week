            <a class="nav-link h5 mb-0 pl-0 " href="#">Contact</a>
          </li>
          <!-- Mypage -->
          <li class="nav-item d-block d-md-none">
            <a class="nav-link h5 mb-0 pl-0 " href="#">Mypage</a>
          </li>
          
          
        </ul>
      </div>
  </nav>
  <%- contents %>
  
