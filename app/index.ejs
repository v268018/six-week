              <small class="text-white">© 2020. All Rights Reserved.</small>
            </div>
            <!-- navbar -->
            <ul class="list-unstyled d-none d-md-flex m-0 align-items-center">
              <li class="mr-l">
                <a href="#"class="text-white">Product</a>
              </li>
              <li class="mr-l">
